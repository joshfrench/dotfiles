[user]
  name = Josh French
  email = josh@vitamin-j.com
[alias]
  co = checkout
  stat = status
  ci = commit
[apply]
  whitespace = nowarn
[push]
  default = upstream
[hub]
  protocol = https
[credential]
  helper = /opt/boxen/bin/boxen-git-credential
[core]
  excludesfile = /opt/boxen/config/git/gitignore
[rerere]
  enabled = true
[init]
	templatedir = ~/.git_template
