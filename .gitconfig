[user]
  name = Josh French
  email = josh.french@rstudio.com
[alias]
  co = checkout
  stat = status
  ci = commit
[apply]
  whitespace = nowarn
[push]
  default = upstream
[hub]
  protocol = https
[core]
	excludesfile = /Users/josh/.gitignore
[rerere]
  enabled = true
[init]
	defaultBranch = main
[credential]
	helper = osxkeychain
[url "ssh://git@github.com/rstudio/"]
  insteadOf = https://github.com/rstudio/
[diff]
	colorMoved = blocks
[color "diff"]
	oldMoved = magenta
	newMoved = yellow
[safe]
	directory = /Users/josh/src/rstudio/connect
